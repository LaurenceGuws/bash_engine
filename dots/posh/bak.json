{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
  "os": "#ACB0BE",
  "pink": "#F5BDE6",
  "lavender": "#B7BDF8",
  "blue": "#8AADF4",
  "wight": "#FFFFFF",
  "text": "#000000",
  "text2": "#ffffff",
  "softGreen": "#A6E3A1",
  "softYellow": "#F9E2AF",
  "softRed": "#F38BA8",
  "softRed2": "#f03469",
  "softBlue": "#89B4FA",
  "softTeal": "#94E2D5",
  "softMaroon": "#EBA0AC",
  "softPeach": "#FAB387",
  "softSky": "#89DCEB",
  "statusText": "#B48EAD",
  
  "rosewater": "#F5E0DC",
  "flamingo": "#F2CDCD",
  "mauve": "#CBA6F7",
  "red": "#F38BA8",
  "maroon": "#EBA0AC",
  "peach": "#FAB387",
  "yellow": "#F9E2AF",
  "green": "#A6E3A1",
  "teal": "#94E2D5",
  "sky": "#89DCEB",
  "lavenderSoft": "#B4BEFE",
  "subtext1": "#A6ADC8",
  "subtext0": "#9399B2",
  "overlay2": "#7F849C",
  "overlay1": "#6C7086",
  "overlay0": "#585B70",
  "surface2": "#45475A",
  "surface1": "#313244",
  "surface0": "#1E1E2E",
  "base": "#1A1826",
  "mantle": "#181825",
  "crust": "#11111B",
  
  "pastelPurple": "#D4B5F9",
  "pastelPink": "#F6BBE7",
  "pastelBlue": "#A0DFFF",
  "pastelGreen": "#BAE8A7",
  "pastelYellow": "#F7E3AF",
  "pastelOrange": "#FFC4A3",
  "pastelRed": "#FFA9B8",
  "pastelTeal": "#9CE5DB",
  "pastelLavender": "#D7C2F9",
  "pastelSkyBlue": "#C1E4FF",
  "pastelMint": "#CFFDE1",
  "pastelPeach": "#FFD8BE",
  "pastelRose": "#FEC7CF",
  "pastelLilac": "#D8B4F5",
  "pastelMaroon": "#E8A9AC"
}
,
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "style": "diamond",
          "template": "@{{ .HostName }}",
          "type": "session"
        },
        {
          "background": "p:green",
          "foreground": "p:text",
          "properties": {
            "folder_icon": "\ue5fe",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "powerline_symbol": "\ue0b4",
          "style": "powerline",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "background": "p:lavender",
          "foreground": "p:text",
          "style": "powerline",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "powerline_symbol": "\ue0b4",
          "template": " {{ .HEAD }}",
          "type": "git"
        },
        {
          "background": "p:softGreen",
          "foreground": "p:text",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "template": "{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "type": "node"
        },
        {
          "background": "p:softYellow",
          "foreground": "p:text",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "type": "python"
        },
        {
          "background": "p:softRed",
          "foreground": "p:text",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "java"
        },
        {
          "background": "p:softBlue",
          "foreground": "p:text",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "template": "{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}",
          "type": "dotnet"
        },
        {
          "background": "p:softTeal",
          "foreground": "p:text",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "go"
        },
        {
          "background": "p:softMaroon",
          "foreground": "p:text",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}",
          "type": "rust"
        },
        {
          "background": "p:softPeach",
          "foreground": "p:text",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "template": "{{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "type": "aws"
        },
        {
          "background": "p:mauve",
          "foreground": "p:text",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "template": "{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:pastelGreen",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "‚ùØ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": false,
  "version": 3
}

